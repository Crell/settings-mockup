{
    "name": "crell/settings-prototype",
    "require": {
        "php": "~8.1",
        "symfony/dependency-injection": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpbench/phpbench": "^1.1",
        "phpstan/phpstan": "^1.0",
        "vishnubob/wait-for-it": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Crell\\SettingsPrototype\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crell\\SettingsPrototype\\": "tests",
            "Crell\\SettingsPrototype\\Benchmarks\\": "benchmarks"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
